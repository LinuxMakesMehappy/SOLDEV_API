{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "errorCode": {
      "oneOf": [
        {
          "type": "number",
          "minimum": 0,
          "maximum": 4294967295
        },
        {
          "type": "string",
          "pattern": "^(0x)?[0-9a-fA-F]+$"
        }
      ]
    }
  },
  "required": ["errorCode"],
  "additionalProperties": false
}